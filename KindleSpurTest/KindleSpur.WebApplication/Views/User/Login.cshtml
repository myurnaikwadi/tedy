                              <div class="formDetails">
                                  <button class="signupButton fontClass" ng-click="signupClick()">SIGNUP</button>
                              </div>
                          </div>
                          <div class="signupForm" style="height: 38% !important;margin-bottom: 0;">
                              <h4 class="sigupLabel fontClass">SIGN IN!</h4>
                              <div class="formDetails">
                                  <input class="inputTagStyle fontClass" style="width: 94%;" type="text" placeholder="USERNAME" />
                              </div>
                              <div class="formDetails">
                                  <input class="inputTagStyle fontClass" style="width: 94%;" type="text" placeholder="PASSWORD" />
                              </div>
                              <div class="formDetails">
                                  <button class="signupButton fontClass" ng-click="loginClick()">SUBMIT</button>
                                  <h5 class="forgotPassword fontClass">FORGOT PASSOWORD</h5>
                              </div>

                          </div>
                          <div class="signInWith" id="appBody">
                              <h4 class="signInWithLabel fontClass">SIGN IN WITH</h4>
