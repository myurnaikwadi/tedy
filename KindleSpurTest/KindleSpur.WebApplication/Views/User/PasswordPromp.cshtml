                        <div class="formDetails">
                            <input class="inputTagStyle fontClass" style="width: 94%;" ng-model="loginDetails.confirmPassword" type="password" placeholder="CONFIRM PASSWORD" />
                        </div>
                        <div class="formDetails">
                            <button class="signupButton fontClass" ng-click="savePassword()">SAVE</button>
