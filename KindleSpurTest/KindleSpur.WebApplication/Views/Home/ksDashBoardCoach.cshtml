    <div class="parentBody" ng-controller="ksDashBoardCoachController">
        <div class="applicationTopStrip">
            <top-Main-Strip></top-Main-Strip>
        </div>
        <div class="applicationMiddleStripMain">
            <div class="parentRoleDashboard">
                <div class="parentRoleMenu coachThemeColor">
                    <div class="profileViewMode genericPointerClass">
                        <div class="profileImagePart"></div>
                        <div class="profileNamePart fontClass genericPointerClass">WELCOME {{loggedDetail.FirstName+" "+loggedDetail.LastName}}</div>
                    </div>
                    <div class="leftSideMenuMainDashBoard">
                        <div class="leftSideMenuParent">
                            <div class="leftSideMenu coachColor" ng-repeat="option in leftSideMenus" ng-click="menuClick($index,option)">
                                <h5 class="leftSideMenuLable fontClass genericPointerClass">{{option.name}}</h5>
                                <h5 ng-if="option.name == 'Add Skills'" style="float: right;font-size: 25px;margin: 1%;" class="leftSideMenuLable fontClass genericPointerClass">+</h5>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="parentRoleDetailRight" ng-if="selectedMenu == '6'">
                    <ctc-Role role="coach" skill-Required="true"></ctc-Role>
                </div>
                <div class="parentRoleDetailRight" ng-if="selectedMenu == '2'">
                    <div class="homeLabelInChat">
                        <h5 class="fontClass">NOTIFICATIONS</h5>
                    </div>

                    <div class="communicationMainPanel">
                        <div ng-show='askFeedback' class="feedbackPanel">
                            <!-- Screen 1 -->
                            <div class="popupContainer" ng-show="formValue == '3'">
                                <div class="popupContainerOne">
                                   
                                    <img src="~/Images/communication/lineDiag1.png" />
                                </div>
                                <div class="popupContainerTwo">
                                    <h1>What is the one thing that you like most about us?</h1>
                                </div>
                                <div class="popupContainerThree">
                                    <textarea ng-model="feedBack.likeMostMessage" placeholder="CLICK HERE TO TYPE MESSAGE"></textarea>
                                </div>
                                <div class="popupContainerFour">
                                    <div class="fa fa-angle-left arrowContainer genericPointerClass" ng-click="formValue = '2'"></div>
                                    <div class="fa fa-angle-right arrowContainer genericPointerClass" ng-click="formValue = '4'"></div>
                                </div>
                            </div>

                            <!-- Screen 2 -->
                            <div class="popupContainer" ng-show="formValue == '1'">
                                <div class="popupContainerOne">
                                    <img src="~/Images/communication/lineDiag2.png" />
                                </div>
                                <div class="popupContainerTwo">
                                    <h1>How Do You Compare the value we provide you against our Competitor</h1>
                                </div>
                                <div class="popupContainerThree">
                                    <img class="genericPointerClass" style="padding: 3%;float:left;" ng-click="feedBack.competitorSelected('1')" ng-class="{ highlightedCompetitor :  feedBack.selectedComparioson == '1'}" src="~/Images/communication/competitorsBetter.png">
                                    <img class="genericPointerClass" style="padding: 3%;float:left;" ng-click="feedBack.competitorSelected('2')" ng-class="{ highlightedCompetitor :  feedBack.selectedComparioson == '2'}" src="~/Images/communication/sameasCompetitors.png">
                                    <img class="genericPointerClass" style="padding: 3%;float:left;" ng-click="feedBack.competitorSelected('3')" ng-class="{ highlightedCompetitor :  feedBack.selectedComparioson == '3'}" src="~/Images/communication/betterCompetitors.png">

                                </div>
                                <div class="popupContainerFour">
                                    <div class="fa"></div>
                                    <div class="fa fa-angle-right arrowContainer genericPointerClass" ng-click="formValue = '2'"></div>
                                </div>
                            </div>

                            <!-- Screen 3 -->

                            <div class="popupContainer" ng-show="formValue == '4'">
                                <div class="popupContainerOne">

                                    <img src="~/Images/communication/lineDiag3.png" />
                                </div>
                                <div class="popupContainerTwo">
                                    <h1>overall customer satisfaction</h1>
                                </div>
                                <div class="popupContainerThree">
                                    <!--	<img style="padding: 3%;float:left;" ng-click="feedBack.starSelected('1')" ng-src="{{ feedBack.selectedstar == '1' && '~/Images/communication/smallSelectedStar.png' || '~/Images/communication/smallStar.png'}}" ng-class="{ highlightedCompetitor :  feedBack.selectedstar == '1'}" src="~/Images/communication/smallStar.png">
                        <img style="padding: 3%;float:left;" ng-click="feedBack.starSelected('2')" ng-src="{{ feedBack.selectedstar == '1' && '~/Images/communication/smallBigSelectedStar.png' || '~/Images/communication/smallBigStar.png'}}" ng-class="{ highlightedCompetitor :  feedBack.selectedstar == '2'}" src="~/Images/communication/smallBigStar.png">
                        <img style="padding: 3%;float:left;" ng-click="feedBack.starSelected('3')" ng-src="{{ feedBack.selectedstar == '1' && '~/Images/communication/bigSelectedStar.png' || '~/Images/communication/bigStar.png'}}" ng-class="{ highlightedCompetitor :  feedBack.selectedstar == '3'}" src="~/Images/communication/bigStar.png">		-->

                                    <img class="genericPointerClass" style="padding: 3%;float:left;" ng-click="feedBack.starSelected('1')" ng-class="{ highlightedCompetitor :  feedBack.selectedstar == '1'}" src="~/Images/communication/smallStar.png">
                                    <img class="genericPointerClass" style="padding: 3%;float:left;" ng-click="feedBack.starSelected('2')" ng-class="{ highlightedCompetitor :  feedBack.selectedstar == '2'}" src="~/Images/communication/smallBigStar.png">
                                    <img class="genericPointerClass" style="padding: 3%;float:left;" ng-click="feedBack.starSelected('3')" ng-class="{ highlightedCompetitor :  feedBack.selectedstar == '3'}" src="~/Images/communication/bigStar.png">
                                </div>
                                <div class="popupContainerFour">
                                    <div class="fa fa-angle-left arrowContainer genericPointerClass" ng-click="formValue = '3'"></div>
                                    <button type="submit" ng-click="feedBack.sendFeedBackDetail()">Submit</button>
                                </div>
                            </div>

                            <!-- Screen 4 -->

                            <div class="popupContainer" ng-show="formValue== '2'">
                                <div class="popupContainerOne">
                                    <img src="~/Images/communication/lineDiag4.png">
                                </div>
                                <div class="popupContainerTwo">
                                    <h1>What do you find most attractive about our company?</h1>
                                </div>
                                <div class="popupContainerThree">

                                    <img class="genericPointerClass" style="padding: 3%;float:left;" ng-click="feedBack.findAttractiveSelected('1')" ng-class="{ highlightedCompetitor :  feedBack.selectedAttractive == '1'}" src="~/Images/communication/price_feedBack.png">
                                    <img class="genericPointerClass" style="padding: 3%;float:left;" ng-click="feedBack.findAttractiveSelected('2')" ng-class="{ highlightedCompetitor :  feedBack.selectedAttractive == '2'}" src="~/Images/communication/product_feedBack.png">
                                    <img class="genericPointerClass" style="padding: 3%;float:left;" ng-click="feedBack.findAttractiveSelected('3')" ng-class="{ highlightedCompetitor :  feedBack.selectedAttractive == '3'}" src="~/Images/communication/customer_service_feedBack.png">
                                    <img class="genericPointerClass" style="padding: 3%;float:left;" ng-click="feedBack.findAttractiveSelected('4')" ng-class="{ highlightedCompetitor :  feedBack.selectedAttractive == '4'}" src="~/Images/communication/location_feedBack.png">
                                </div>
                                <div class="popupContainerFour">
                                    <div class="fa fa-angle-left arrowContainer genericPointerClass" ng-click="formValue = '1'"></div>
                                    <div class="fa fa-angle-right arrowContainer genericPointerClass" ng-click="formValue = '3'"></div>
                                </div>
                            </div>
                        </div>
                        <div class="conversationPanel">
                            <div class="conversationPanelDiv">
                                <div class="conversationList " ng-click="conversationLoad($index,conversation)" ng-repeat="conversation in conversationList">
                                    <div class="conversationListPanel genericPointerClass" ng-class="{ highlightedGrayClass : conversation.selectedConversation == true }">
                                        <div style="float:left;    margin: auto auto;width: 85%;">
                                            <h5 style="margin:auto auto;margin-left: 0;font-size: large;float: left;width: 88%;    font-weight: 700;    font-size: 15px;">{{conversation.name}}</h5>
                                            <div class="applicationNotiCount" title="5" style=" display flex;float:left;text-align: center;height 15px;width 15px;">
                                                <h6 class="fontClass" style=" font-size 10px;display inline-block;text-align center; text-overflow clip; margin auto auto;color:white;">5</h6>
                                            </div>
                                        </div>
                                    </div>
                                    <div ng-show="conversation.selectedConversation == true" class="triangleArrowRight trangleOveRideBlack" style="float: left;margin-top: 15px;"></div>
                                </div>
                            </div>
                        </div>
                        <div class="communicationPanel">
                            <div class="chatParent">
                                <div class="chatHeader">
                                    <div class="chatStrongHeader">
                                        <div class="chatHalfContainer">
                                            <span class="chatHalfContainerSpan">
                                                <h5>Paresh Patil</h5>
                                            </span>
                                            <span class="chatHalfContainerSpanNxt">
                                                <h6 class="genericPointerClass">Close Session</h6>
                                                <h6 class="genericPointerClass" ng-click="askFeedBackFunc()">Ask Feedback</h6>
                                            </span>
                                        </div>
                                        <div class="chatHalfContainer">
                                            <span class="chatHalfContainerSpanNxt" style="align-items: center;">
                                                <h6 style="padding: 0px;">UI-UX Designer | PUNE AREA | INDIA</h6>
                                            </span>
                                        </div>
                                    </div>


                                </div>
                                <div class="chatMiddle">
                                    <div class="chatOuterContainer">
                                        <div class="chatSender">
                                            <h5>I would like to request couching from you.</h5>
                                        </div>
                                        <div class="chatTime">
                                            <h5>02/20</h5>
                                        </div>
                                    </div>
                                    <div class="chatOuterContainer">
                                        <div class="chatReceiver">
                                            <h5>Couching Request from Paresh has been accepted</h5>
                                        </div>
                                        <div class="chatTime">
                                            <h5 style="text-align : right">02/20</h5>
                                        </div>
                                    </div>
                                </div>
                                <div class="chatBottom">
                                    <div class="chatBottomOne">
                                        <textarea placeholder="CLICK HERE TO TYPE MESSAGE"></textarea>
                                    </div>
                                    <div class="chatBottomTwo">
                                        <div class="iconContainer">
                                            <div class="fa fa-paperclip" style="padding: 0px 8px;"></div>
                                            <div class="fa fa-phone" style="padding: 0px 8px;"></div>
                                        </div>
                                        <div class="iconContainer" style="justify-content: center;">
                                            <button type="submit">Submit</button>
                                        </div>

                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>


                </div>
                <div class="parentRoleDetailRight" ng-if="selectedMenu == '1'">
                    <div class="homeLabelInMainDB">
                        <h5 class="fontClass">COACHING STATUS</h5>
                    </div>
                    <div class="mainDashBoardWizardParent">
                        <div class="statusParentDiv">
                            <div class="ngRepeatDivForStatus shadow4" ng-repeat="option in coachingStatusArray">

                                <div class="profileImageCoachingStatus">
                                    <img src="" style="margin: auto auto;float: left;height: 45px;width: 45px;" />
                                </div>
                                <div class="profileNameCoachingStatus">
                                    <h5 class="textAlignCoachingStatus fontClass">{{option.name}}</h5>
                                </div>
                                <div class="skillsCoachingStatus">
                                    <h5 class="textAlignCoachingStatus fontClass" style="font-weight: 500;">{{option.skills}}</h5>
                                </div>
                                <div class="progressBarCoachingStatus">
                                    <div style="margin: auto auto;float: left;width: 100%; height: 18px; border: 1px solid #f6f6f6;">
                                        <div style="height: 100%; width: {{option.progressBar+'%'}};background: #F1201B;"></div>
                                    </div>
                                </div>
                                <div class="statusCoachingStatus">
                                    <h5 class="fontClass" style="color:#A9A9A9;margin: auto auto;float: left;margin-left: 0;">{{option.status}}</h5>
                                </div>
                                <div class="treeCoachingStatus">
                                    <img src="" style="margin: auto auto;float: left;height: 45px;width: 45px;" />
                                </div>
                                <div class="communicationsCoachingStatus">
                                    <img src="" class="iconOnStatusArray">
                                    <img src="" class="iconOnStatusArray">
                                    <img src="" class="iconOnStatusArray">
                                </div>
                            </div>

                        </div>
                    </div>
                    <div class="applicationBottomStrip">
                        <bottom-Main-Strip></bottom-Main-Strip>
                    </div>	
                </div>
                <div class="parentRoleDetailRight" ng-if="selectedMenu == '0'">
                    <div class="userNotificationParent">
                        <div class="userRoleWizardParent" style="display: block;overflow: auto;float:left;">
                            <div class="notificationWiz " ng-repeat="notification in notifications">
                                <div ng-if="notification.notificationType == '1'">
                                    <h6 class="notificationWizReason">{{notification.name}}</h6>
                                    <h4 class="notificationWizName">{{notification.assignPerson}}</h4>
                                    <div class="notificationWizButton">
                                        <button style="width:100%;font-size: 10px;padding: 5px;font-weight: bolder;" class="signupButton fontClass genericPointerClass">ACCEPT</button>
                                    </div>
                                </div>
                                <div ng-class="{ mentorThemeColor : notification.role =='mentor',coacheeThemeColor : notification.role =='coachee'}" style="float: left;width: 100%;height: 100%;" ng-if="notification.notificationType == '2'">
                                    <div class="notificationWizReason" style=" margin 4%;border:0px;color: white; padding 0;">{{notification.name}}</div>
                                    <div class="notificationTimingPanel">
                                        <h6 class="notificationTiming notificationDate">{{notification.meetingDate}}</h6>
                                        <h6 class="notificationTiming notificationTime">{{notification.meetingTime}}</h6>
                                        <h6 style="float:right;    margin-right: 8%;" class="notificationTiming notificationDiff">{{notification.meetingTimeDiff}}</h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="dashBoardWizard">
                        <div class="homeLabelInMainDB">
                            <h5 class="fontClass">DASHBOARD >></h5>
                        </div>
                        <div class="mainDashBoardWizardParent">
                            <div class="dashBoardMenuCoach">
                                <div class="dashBoardMenuCoachRight genericPointerClass" ng-click="selectedOption($index,option)"  ng-repeat="option in rightSideDashBoardArray">
                                    
                                    <div class="flipContainer" ng-class="{ flipper : option.selectedOption == true }" ng-click="flipClass()">
                                        <div class="flipper" style="float: left; width : 100%; height : 100%;">
                                            <div class="front">
                                                <!-- front content -->

                                                <div class="flipDivOnClass">
                                                    <div class="promoCodeClass">
                                                        <h5 style="font-size: 18px; margin: 5px;" ng-model="gameKey"></h5>
                                                        @*<h5 style="font-size: 18px; margin: 5px;">12345</h5>*@
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="back" ng-class="{ disableClass : option.name == 'BRAIN GAMES'}">
                                                <!-- back content -->
                                                <div class="dashBoardMenuCoachRightCircle">
                                                    <div class="dashBoardMenuCoachRightCirclePart"></div>
                                                </div>
                                                <div class="dashBoardMenuCoachRightlabel">
                                                    <h5 style="color:black;margin: auto;" class="leftSideMenuLable fontClass">{{option.name}}</h5>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                        <div class="applicationBottomStrip">
                            <bottom-Main-Strip ></bottom-Main-Strip>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
 
