<div class="parentBody" ng-controller="ksStateHomeController">
        <div style="position:absolute;z-index:1;background: white;width: 30%;border: 1px solid #ddd;height: 18%;" class="hiddenErrorNotificationClass" ng-class="{ 'showErrorNotificationClass' : displayAlert.idleAlert.showAlert  == true}">
              <div style="float: left;width: 100%;height: 100%;position: relative;" ng-show="displayAlert.idleAlert.formatType == '1'">
                  <div style="float: left;width: 100%;height: 45%;position: relative;display: flex;">
                      <h5 style="float: left;width: 90%;text-align: center;position: relative;margin: auto;margin-bottom: 0;font-size: 17px;">You're Away From platform. Please Do Something!</h5>
                  </div>
                  <div idle-countdown="countdown" ng-init="countdown=5" style="float: left;width: 100%;height: 55%;position: relative;">
                      <div class="uploadedDataHeading">
                          <progress class="progressBarUploadIdle" style="transition:all 0.7s ease" min="0" max="10" value="{{countdown}}"></progress>                                                      
                      </div>  
                      <h5 style="float: left;width: 100%;text-align: center;position: relative;" class="alert-message1"> You'll be logged out in {{countdown}} second(s).</h5>                    
                  </div>
              </div>
              <div ng-show="displayAlert.idleAlert.formatType == '2'">
                  <div class="modal-header">
                      <h3>You've Timed Out!</h3>
                  </div>
                  <div class="modal-body">
                      <p>
                          You were idle too long. Normally you'd be logged out, but in this demo just do anything and you'll be reset.
                      </p>
                  </div>
              </div>
        </div>
        <div ng-if="loadApplicationLevel.loadDiv == true" class="sweet-alert" style="margin:0;position:absolute;z-index:1;background: transparent;width: 100%;height: 100%;display:flex;" ng-show="loadApplicationLevel.loadDiv">
            <div style="float: left;width: 320px;height: 245px;background: white;position: relative;margin:auto;padding: 5px;border-radius: 10px;box-shadow: 0px 0px 10px 2px;">
                    <div style="float: left;width: 100%;height: 100%;position: relative;">
                    <div class="flip-DataFront" ng-class="{ flipper : loadApplicationLevel.loadAnimation == true}">
                        <div class="front" style="background: transparent;">

                        </div>
                        <div class="back" style="background: white;">
                            
                            <div class="fullWidthHeightClass"  ng-if="loadApplicationLevel.positiveResponse==false && loadApplicationLevel.negativeResponse== false">
                                <div style="float: left;width: 100%;height: 45%;position: relative;display: flex;">
                                    <div class="icon warning pulseWarning" style="display: block;width: 60px;height: 60px;"> 
                                        <span class="bodySpanAlert pulseWarningIns" style="height: 29px;"></span> 
                                        <span class="dotSpanAlert pulseWarningIns"></span>
                                    </div>
                                    @*<h5 class="fontClass" style="float: left;position: relative;margin: auto;margin-left: 5%;font-size: 17px;">{{loadApplicationLevel.alertType}}</h5>
                                    <img class="genericPointerClass" ng-click="loadApplicationLevel.loadAnimation=false;loadApplicationLevel.loadDiv=false;" style="width: 10px;height: 10px;float:left;position: relative;margin: auto;margin-right: 2%;" title="Close" src="~/Images/cancel.png" />*@
                                    
                                </div>
                                <div style="float: left;width: 100%;height: 35%;position: relative;">
                                    <div style="float: left;width: 100%;height: 50%;position: relative;display:flex;">
                                        <h5 class="fontClass" style="float: left;width: 100%;text-align: center;position: relative;margin: auto;font-size: 33px;">Are you sure ?</h5>
                                    </div>
                                    <div style="float: left;width: 100%;height: 50%;position: relative;display:flex;">
                                        <h5 class="fontClass" style="float: left;width: 100%;text-align: center;position: relative;margin: auto;font-size: 12px;">{{loadApplicationLevel.alertMessage}}</h5>
                                    </div>
                                </div>
                                <div style="float: left;width: 100%;height: 20%;position: relative;display: flex;">
                                    <button ng-style="{ 'color': button.color, 'border' : ('1px solid '+button.color) }" ng-repeat="button in loadApplicationLevel.buttonArray" ng-click="alertButtonAction({ event : $event,button : button,index : $index })" style="text-transform: uppercase;height: 25px;background-color: white;font-size: 10px;padding: 5px;margin: auto;" class="signupButton fontClass">{{button.buttonName}}</button>                                    
                                </div>
                            </div>
                            <div class="fullWidthHeightClass" ng-if="loadApplicationLevel.positiveResponse==true && loadApplicationLevel.negativeResponse== false">
                                <div class="icon success" style="display: block;">
                                    <span class="line tip animateSuccessTip"></span>
                                    <span class="line long animateSuccessLong"></span>
                                    <div class="placeholder"></div>
                                    <div class="fix"></div>
                                </div>
                                <div style="float: left;width: 100%;height: 25%;position: relative;display:flex;">
                                    <h5 class="fontClass" style="float: left;width: 100%;text-align: center;position: relative;margin: auto;font-size: 33px;">Deleted</h5>
                                </div>
                                <div style="float: left;width: 100%;height: 24%;position: relative;display: flex;">
                                    <button ng-style="{ 'color': 'green', 'border' : ('1px solid #A5DC86') }" ng-click="loadApplicationLevel.loadAnimation=false;loadApplicationLevel.loadDiv=false;loadApplicationLevel.dd=false;" style="text-transform: uppercase;height: 25px;background-color: white;font-size: 10px;padding: 5px;margin: auto;" class="signupButton fontClass">Ok</button>
                                </div>

                            </div>
                            <div class="fullWidthHeightClass" ng-if="loadApplicationLevel.negativeResponse==true && loadApplicationLevel.positiveResponse==false">
                                <div class="icon error animateErrorIcon" style="display: block;"><span class="x-mark animateXMark"><span class="line left"></span><span class="line right"></span></span></div>
                                <div style="float: left;width: 100%;height: 25%;position: relative;display:flex;">
                                    <h5 class="fontClass" style="float: left;width: 100%;text-align: center;position: relative;margin: auto;font-size: 33px;">Cancelled</h5>
                                </div>
                                <div style="float: left;width: 100%;height: 24%;position: relative;display: flex;">
                                    <button ng-style="{ 'color': 'green', 'border' : ('1px solid #A5DC86') }" ng-click="loadApplicationLevel.loadAnimation=false;loadApplicationLevel.loadDiv=false;loadApplicationLevel.dd=false;" style="text-transform: uppercase;height: 25px;background-color: white;font-size: 10px;padding: 5px;margin: auto;" class="signupButton fontClass">Ok</button>
                                </div>
                            </div>
                          
                           
                        </div>
                    </div>
                </div>
            </div>
            
        </div>
        <div style="position:absolute;z-index:1;" class="hiddenErrorNotificationClass" ng-class="{ 'showErrorNotificationClass' : displayAlert.showAlert  == true}">

            @*Green*@
            <div class="alert alert-success" style="margin-bottom: 0;"  ng-show="displayAlert.formatType == '1'">
                <h5 class="closealert-padding close" ng-click="displayAlert.showAlert = false;">×</h5>
                <h5 class="alert-message1">{{displayAlert.message}}</h5>
            </div>

            @*red*@
            <div class="alert alert-error" style="margin-bottom: 0;" ng-show="displayAlert.formatType == '2'">
                <h5 class="closealert-padding close" ng-click="displayAlert.showAlert = false">×</h5>
                <h5 class="alert-message1">{{displayAlert.message}}</h5>                
            </div>
            <h5 style="position:absolute;height:3px;width:100%; background: red;margin: 0;transition:all 0.8s cubic-bezier(0.73, 0.85, 0.58, 1);" ng-style="{ 'width' : (displayAlert.count +'0%') }"></h5>
        </div>
        <div class="newApplicationStripInvitation" ng-click="$event.stopPropagation();" ng-init="load=false" ng-class="{ AminationStrip : load == true }">
            <div  id="1" title="invite your friends to register" class="roundedCornerShape" ng-click="load=!load;invitation={}">
                <img id="1" title="invite your friends to register" ng-hide="load" src="~/Images/Mail_Send_Email_Envelope.png" />
                <img id="1" ng-show="load" src="~/Images/cancelWhiteIcon.png" />
            </div>
            <div class="stripInvitationBody">
                <div class="fullWidthHeightClass" ng-if="load" outside-click out-Side="closeMyPopup" id-Arr="['1']">
                    <div class="headingInvitation">
                        <h5 style="float: left;margin: auto;color: #333333;">INVITE A FRIEND</h5>
                    </div>
                    <div class="middleMainBody">
                        <div style="float: left;width: 100%;display: flex;height: 30%;">
                            <input ng-if="load" class="inputTagStyle fontClass" style="font-size: 13px;margin: auto;padding:5px;width: 80%;" type="text" ng-model="invitation.email" ng-keyup="checkEmailValidation({ event : $event, email : invitation.email, otherCall : false })" placeholder="Enter email addresses" ng-required />
                        </div>
                        <div style="float: left;width: 100%;display: flex;height: 70%;">
                            <textArea ng-if="load" class="fontClass" style="padding: 5px;margin: auto;resize: none; font-size: 13px;height: 87%;width: 80%;" placeholder="Invite your friends to register. Grow Knowledge and Get Rewarded!" ng-model="invitation.description"></textArea>
                        </div>
                    </div>
                    <div class="buttonSubmit">
                        <button ng-if="load" type="button" ng-click="sendInvitation($event)" style="margin: auto;width: 20%;padding: 0%;height: 22px;font-size: 9px;" class="signupButton fontClass btn btn-default">SEND</button>
                    </div>

                </div>
            </div>
        </div>
        <div ng-click="$event.stopPropagation();" class="defaultProfileContainClass" ng-class="{ loadProfileContain : uiFlag.loadProfileView == true}">
            <div class="profileImageNameTagClass">
                <div style="float:left;width:100%;height:20%;display: flex;text-align: center;">
                    <h5 style="font-size: 19px;margin: auto;margin-bottom: 0%;width: 85%;font-weight: 700;color: rebeccapurple;" class="leftSideMenuLable fontClass eliipsisClass">WELCOME</h5>
                </div>
                <div class="imageConversation" style="width:100%;height: 45%;">
                    <img ng-click="$event.stopPropagation();uiFlag.loadProfileView = false;extraParam.profile($event)" class="genericPointerClass" style="border: 5px solid white;margin:auto auto;margin-bottom: 0;width: 80px;height: 80px;border-radius: 50%;" ng-src="{{loggedDetail.Photo ?  loggedDetail.Photo  : '../../../Images/icons/If no Profile photo.png'}}" />
                </div>
                <div class="profileNamePart fontClass" style="cursor: default;height: 20%;width:100%;display: flex;padding: 0;border-bottom: 2px solid;">
                    <div style="margin:auto auto;width:100%;height:80%;">
                        <div style="float:left;width:100%;height:60%;display: flex;text-align: center;">
                            <h5 ng-click="$event.stopPropagation();uiFlag.loadProfileView = false;extraParam.profile($event)" style="font-size: 19px;margin: auto;margin-bottom: 0%;width: 85%;" class="leftSideMenuLable fontClass eliipsisClass genericPointerClass">{{(loggedDetail.FirstName) | uppercase }}</h5>
                        </div>
                        <div style="float:left;width:100%;height:40%;display: flex;text-align: center;">
                            <h5 style="font-size: 11px;margin: auto;width: 93%;" class="leftSideMenuLable fontClass eliipsisClass">{{(loggedDetail.EmailAddress)}}</h5>
                        </div>
                    </div>
                </div>
                <div class="profileNamePart fontClass" style="height: 15%;width:100%;display: flex;padding: 0;">
                    <h5 style="font-size: 13px;margin: auto;" ng-click="$event.stopPropagation();uiFlag.loadProfileView = false;extraParam.profile($event)" class="leftSideMenuLable fontClass eliipsisClass genericPointerClass">PROFILE</h5>
                    <h5 style="font-size: 13px;margin: auto;" ng-click="$event.stopPropagation();uiFlag.loadProfileView = false;extraParam.logout($event)" class="leftSideMenuLable fontClass eliipsisClass genericPointerClass">LOGOUT</h5>
                </div>
            </div>
        </div>
        <div ng-click="uiFlag.loadBottomContain=false;" ng-class="{ loadBottomContain : uiFlag.loadBottomContain == true}" style="overflow: hidden;display:none; position:absolute;width:100%;height:100%;z-index: 3;">
            <div class="fullWidthHeightClass aboutUsParent fontClass" ng-include src="extraParam.templateUrl"></div>
        </div>
        <div ng-click="uiFlag.loadRepository=false;uiFlag.loadModule = null" ng-class="{ loadRepository : uiFlag.loadRepository == true}" style="overflow: hidden;display:none; position:absolute;width:100%;height:100%;">
            <div class="fullWidthHeightClass" style="display: flex;" ng-click="$event.stopPropagation()">
                <common-Repository ng-if="uiFlag.loadModule == 'Attachment'" extra-Param="extraParam" style="width:75%;height:85%;margin:auto;"></common-Repository>
                <schedule-Meeting ng-if="uiFlag.loadModule == 'Meeting'" extra-Param="extraParam" style="width:50%;height:60%;margin:auto;"></schedule-Meeting>
                <profile-Page ng-if="uiFlag.loadModule == 'Profile'" load-Outside="true" user-Info="extraParam.userInfo" close-Popup="closeCallBack()" edit-Required="false" style="width:80%;height:80%;margin:auto;position: relative;background:white;overflow:auto;"></profile-Page>
            </div>
        </div>
        <div class="applicationTopStrip" ui-view="topMainPanel"></div>
        <div class="applicationMiddleStrip" ui-view="mainBody"></div>
        <div class="applicationBottomStrip" ui-view="bottomPanel"></div>
    </div>