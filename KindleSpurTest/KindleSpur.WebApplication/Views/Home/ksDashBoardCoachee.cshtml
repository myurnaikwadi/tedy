                                            <div ng-style="icon.styleObj" style="position: absolute;" class="" ng-repeat="icon in feedbackDisplayIcon">
                                               <h6 title="To see details please click on it" ng-if="icon.replaceName == null && expandIndex != $index" ng-style="icon.style" class="fontClass genericPointerClass feedBackPanelButton" ng-click="iconCLicked($event,$index,icon)">{{icon.Name}}</h6>
                                                                            
                                                 <div style="float:left;width:100%;height:100%;border:1px solid #a9a9a9;border-right: 0;border-bottom: 0;" ng-if="expandIndex == $index && icon.Name == 'F'">
