<div class="parentBody" ng-controller="ksDashBoardMenteeController">
    <div class="applicationTopStrip">
        <top-Main-Strip></top-Main-Strip>
    </div>
    <div class="applicationMiddleStripMain">
        <div class="parentRoleDashboard">
            <div class="parentRoleMenu menteeThemeColor">
                <div class="profileViewMode genericPointerClass">
                    <div class="profileImagePart"></div>
                    <div class="profileNamePart fontClass genericPointerClass">WELCOME {{loggedDetail.FirstName+" "+loggedDetail.LastName}}</div>
                </div>
                <div class="leftSideMenuMainDashBoard">
                    <div class="leftSideMenuParent">
                        <div class="leftSideMenu MenteeColor" ng-repeat="option in leftSideMenus" ng-click="menuClick($index,option)">
                            <h5 class="leftSideMenuLable fontClass genericPointerClass">{{option.name}}</h5>
                            <h5 ng-if="option.name == 'Add Topics'" style="float: right;font-size: 25px;margin: 1%;" class="leftSideMenuLable fontClass genericPointerClass">@*+*@</h5>
                        </div>
                    </div>
                </div>
            </div>

            <div class="parentRoleDetailRight" ng-show="selectedMenu == '5'">
                <div class="ctsParent">
                    <div class="ctsParentMain">
                        <div class="ctsParentMainPanel">
                            <div class="ctsCategoryHeading row">
                                <div class="col-md-8 search-result">
                                    <h3 class="margin-small">Search Results</h3>
                                </div>
                                <div class="col-md-2 search-mentor">
                                    <div class="inner-addon left-addon">
                                        <span ng-show="searchKey.length > 0" class="filter-icon" ng-click="clearSearch()"><i class="fa fa-times-circle"></i></span>
                                        <span ng-show="searchKey.length == 0" class="filter-icon"><i class="fa fa-search"></i></span>
                                        <input type="text" class="auto-complete" cts-dropdown ng-model="searchKey" key="searchKey" searching="searching" skill="selectedSkill" placeholder="Type to search" />
                                    </div>
                                    <div class="auto-complete-container" ng-show="searching" ng-class="{hide:!searching}">
                                        <div class="item" ng-repeat="skill in skills = (availableSkills | filter: skillFilter)">
                                            <div ng-click="selectSkill(skill)">{{skill.Name}}</div>
                                        </div>
                                        <div ng-show="!skills.length && searching" class="no-result">
                                            No result found.
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="ctsCategoryPanelMainPanel" style="overflow-y:scroll;">
                                <h5 class="margin-small">Recommended Coaches</h5>
                                <div class="filterListContainer">
                                    <div class="mentor-list">
                                        <div class="row" style="margin:0px;">
                                            <div class="mentor-card" ng-repeat="coach in Coaches" cts-mentor-card>
                                                <div class="mentor-info">
                                                    <span class="profile-close" style="float:right;top:10px;right:10px;"><i class="fa fa-times"></i></span>
                                                    <div class="profile-image">
                                                        <img src="https://www.abeautifulsite.net/content/uploads/2014/08/rounded-image-250x250.png" class="img-circle" />
                                                    </div>
                                                    <div class="profile-info">
                                                        <p class="mentor-name">{{coach.Name}}</p>
                                                        <p class="mentor-skils">{{coach.Skill}}</p>
                                                        <p class="mentor-location">{{coach.City}} <b>|</b> {{coach.Country}}</p>
                                                    </div>
                                                </div>
                                                <div class="mentor-actions" style="display:none; position:absolute">
                                                    <div class="row">
                                                        <i class="btn col-md-2 fa fa-linkedin btn-blue" style="border-radius:0px;"></i>
                                                        <button class="btn col-md-9 btn-gray" data-toggle="modal" data-target="#myModal" style="border-radius:0px;">View Profile</button>
                                                    </div>
                                                    <br />
                                                    <button class="btn signupButton">Request</button>
                                                    <br />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="applicationBottomStrip">
                        <bottom-main-strip></bottom-main-strip>
                    </div>
                </div>
            </div>

            <div class="parentRoleDetailRight" ng-if="selectedMenu == '0'">
                <div class="userNotificationParent">
                    <div class="userRoleWizardParent" style="display: block;overflow: auto;float:left;">
                        <div class="notificationWiz " ng-repeat="notification in notifications">
                            <div ng-if="notification.notificationType == '1'">
                                <h6 class="notificationWizReason">{{notification.name}}</h6>
                                <h4 class="notificationWizName">{{notification.assignPerson}}</h4>
                                <div class="notificationWizButton">
                                    <button style="width:100%;font-size: 10px;padding: 5px;font-weight: bolder;" class="signupButton fontClass genericPointerClass">ACCEPT</button>
                                </div>
                            </div>
                            <div ng-class="{ mentorThemeColor : notification.role =='mentee',coacheeThemeColor : notification.role =='coachee'}" style="float: left;width: 100%;height: 100%;" ng-if="notification.notificationType == '2'">
                                <div class="notificationWizReason" style=" margin 4%;border:0px;color: white; padding 0;">{{notification.name}}</div>
                                <div class="notificationTimingPanel">
                                    <h6 class="notificationTiming notificationDate">{{notification.meetingDate}}</h6>
                                    <h6 class="notificationTiming notificationTime">{{notification.meetingTime}}</h6>
                                    <h6 style="float:right;    margin-right: 8%;" class="notificationTiming notificationDiff">{{notification.meetingTimeDiff}}</h6>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="dashBoardWizard">
                    <div class="homeLabelInMainDB">
                        <h5 class="fontClass">DASHBOARD >></h5>
                    </div>
                    <div class="mainDashBoardWizardParent">
                        <div class="dashBoardMenuCoach">
                            <div class="dashBoardMenuCoachRight genericPointerClass  col-md-3" ng-repeat="option in rightSideDashBoardArray" ng-class="{'dashBoardMenuCoachBottom': $index == 3}">
                                <div class="dashBoardMenuCoachRightCircle">
                                    <div class="dashBoardMenuCoachRightCirclePart">
                                        <img src={{option.url}} class="dashBoardMenuCoachImage">
                                    </div>
                                </div>
                                <div class="dashBoardMenuCoachRightlabel">
                                    <h5 style="color:black;margin: auto;" class="leftSideMenuLable fontClass">{{option.name}}</h5>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="applicationBottomStrip">
                        <bottom-Main-Strip></bottom-Main-Strip>
                    </div>
                </div>
            </div>
            <div class="parentRoleDetailRight" style="padding:1%;padding-bottom: 0;" ng-if="selectedMenu == '4'">
                <rss-Feed ng-if="feedCategoryArray.length > 0" skill="feedCategoryArray"></rss-Feed>
            </div>
        </div>
    </div>
</div>

