                                    <img style="width: 20px;float: left;height: 20px;background: white;margin: auto 0;margin-left: 1%;" ng-src="{{organisation}}" ng-click="editDesignation = true">
                                </div>
                                <div ng-show="editModeProfile" class="designationProfile">

                                    <div style="margin: auto 0;" class="formDetails">
                                        <input style="padding:0;" class="inputTagStyle fontClass widthForFirstNameProfile" type="text" placeholder="Designation" />
                                        <input class="inputTagStyle widthForFirstNameProfile fontClass" style="padding:0;margin-left: 2%;" type="text" placeholder="Company Name" />

                                    </div>
                                </div>*@
                            <div class="emailmobileProfile">
                                <div class="subParentEmailMobile">
                                    <div style="float: left;width: 100%;height: 25px;display: flex;">
                                        <h5 style="float: left;width: 15%;margin: auto 0;color: #333333;">Email</h5>
                                        <h5 style="float: left;width: auto;max-width: 30%;text-overflow: ellipsis;margin: auto 0;color: #333333;">{{userInfo.EmailAddress}}</h5>
                                    </div>
                                    <div style="float: left;width: 100%;height: 25px;display: flex;">
                                        <h5 style="float: left;width: 15%;margin: auto 0;color: #333333;">Mobile</h5>
                                        <h5 ng-hide="editModeProfile" style="float: left;width: auto;max-width: 30%;text-overflow: ellipsis;margin: auto 0;color: #333333;">{{myInfo.mobileNumber ? '+91'+myInfo.mobileNumber : 'Not Specified'}}</h5>

                                        <input ng-show="editModeProfile" ng-model="myInfo.mobileNumber" class="inputTagStyle widthForFirstNameProfile fontClass" style="padding:5px;width: 16%;" type="text" placeholder="Mobile Number" />
                                    </div>
                                </div>

                            </div>
                            <div class="linkedInLinkProfile">

                                <img style="width: 20px;float: left;height: 20px;background: white;margin: auto 0;" src="~/Images/linkedin_square_gray.png" />
                                <h5 ng-hide="editModeProfile" style="float: left;margin: auto 0;color: #333333;margin-left: 1%;">{{myInfo.linkedInLink ? myInfo.linkedInLink : 'Not Specified'}}</h5>
                                <input ng-show="editModeProfile" ng-model="myInfo.linkedInLink" class="inputTagStyle widthForFirstNameProfile fontClass" style="padding:5px; float: left;margin: auto 0;color: #333333;margin-left: 1%;" type="text" placeholder="linkedIn link" />
                            </div>
                            <img ng-click="editModeProfile = false" ng-show="editModeProfile" style="width: 15px;float: right;height: 15px;background: white;margin: auto 0;margin-left: 1%;right: 2%;bottom: 0;position: absolute;cursor:pointer;" title="Cancel" src="~/Images/cancel.png" />
                            <img ng-click="sendDetailsToServer()" ng-show="editModeProfile" style="width: 15px;float: right;height: 15px;background: white;margin: auto 0;margin-left: 1%;right: 7%;bottom: 0;position: absolute;" title="Save" src="~/Images/save.png" />
