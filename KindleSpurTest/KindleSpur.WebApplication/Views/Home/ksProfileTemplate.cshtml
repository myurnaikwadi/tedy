<div class="parentBody" style="display:flex;">

    <div class="applicationProfilePanel" style="style="display:none;position : relative;width : 70%;height: 100%;float : left;margin:auto;">
        <div class="" style="position : relative;width : 100%;height: 40%;float : left;z-index: 1;">
            <img style="width: 100%; height: 100%;float: left;padding: 5px;border: 3px solid lightgray;" ng-src="{{ myInfo.profileBackgroundImage ? myInfo.profileBackgroundImage : '../../../Images/userprofile.png'}}" />

            <div class="widthClassss">
                <img style="width: 130px; height: 130px;float: left;border-radius:100%;border: 3px solid white; position: absolute;bottom: 30px;padding: 2px;left: 51px;" ng-src="{{ myInfo.profileImage ? myInfo.profileImage : '../../../Images/icons/If no Profile photo.png'}}" />
                <h5 style="position: absolute;bottom: 11px;padding: 2px;left: 192px;color: white;font-size: 27px;">{{ (myInfo.firstName+" "+myInfo.lastName) | uppercase }}</h5>
            </div>

        </div>
        <div class="" style="position : relative;width : 100%;height: 60%;float : left;">
            <div class="stripForMenuProfile" style="position : relative;width : 75%;margin-left:25%;height: 13%;float : left;display:flex;">
                <div style="display:flex;margin:auto;margin-left: 0;margin-right: 5%;width:20%;height:100%;" ng-click="selectedMenuProfile($index)" ng-class="{ selectedMenu : selectedMenuIndex == $index }" ng-repeat="menu in profileMenuArr">
                    <a style="float:left;margin:auto;color: black;" ng-class="{ selectedMenu : selectedMenuIndex == $index }" href="#carousel" data-slide-to="{{$index}}">{{menu.name}}</a>
                </div>                
            </div>
            <div class="optionLeftPanelProfile" style="position : relative;width : 25%;height: 88%;float : left;">

                <div style="position:relative;width:85%; height:56%; padding: 8px;margin:auto;margin-top:5%;box-shadow: 0 0 19px 0px #969595;border-radius: 12px;">
                    <div class="podcastHeader" style="height: 20%;">
                        <h5 style="margin: auto;font-weight: bold;color:#696565;" class="fontClass ValueFeedsHeade">About Me</h5>
                    </div>
                    <div class="descriptionLabelMessage">
                        <p ng-hide="editDescription" style="color:#696565;width: 100%;padding: 8px;max-height: 100%;" class="descriptionMessage">
                            {{myInfo.description}}
                        </p>
                    </div>
                </div>
                <div style="position:relative;width:85%; height:30%; padding: 8px;margin:auto;    margin-top: 10%;box-shadow: 0 0 19px 0px #969595;border-radius: 12px;">
                    <div class="podcastHeader" style="height: 33%;">
                        <h5 style="margin: auto;font-weight: bold;color:#696565;" class="fontClass ValueFeedsHeade">Social</h5>
                    </div>
                    <div class="descriptionLabelMessage" style="height: 67%;">
                        <img style="width: 20px;float: left;height: 20px;background: white;margin: auto 0;" src="~/Images/linkedin_square_gray.png" />
                        <h5 style="float: left;margin: auto 0;color: #696565;margin-left: 1%;font-size: 12px;">{{myInfo.linkedInLink ? myInfo.linkedInLink : 'Not Specified'}}</h5>
                    </div>
                </div>

            </div>

            <div class="rightPanelProfile" style="position : relative;width : 75%;height: 88%;float : left;">
                <div id="carousel" class="carousel slide fullWidthHeightClass" data-slide-to="carousel" data-interval="false">
                    <!-- Wrapper for slides -->
                    <div class="carousel-inner fullWidthHeightClass" role="listbox">
                        <div class="item active fullWidthHeightClass">
                            <cube-Strct class="cubeParentClass" ng-show="loadingObject.showLoading" loading-Object="loadingObject"></cube-Strct>
                            <div class="fullWidthHeightClass" ng-hide="loadingObject.showLoading">
                                <div style="float: left;width: 100%;height: 100%;position: relative;" class="col-md-12">
                                    <div class="podcastsContainer col-md-5 col-sm-12">
                                        <div class="podcastHeader" style="border: 1px solid #ddd;height: 15%;box-shadow: 0 0 5px 0px #969595;margin: 7px;margin-bottom: 0;">
                                            <h5 style="margin: auto;margin-left: 2%;" class="fontClass ValueFeedsHeade">Personal Information</h5>     
                                        </div>
                                        <div class="ImpactZoneChildDiv heightZeroClassProfile" ng-class="{ heightAnimationProfile : animationActicvate == true }" style="border: 1px solid #ddd;margin: 7px;margin-top: 0;box-shadow: 0 0 5px 0px #969595;">
                                            <div class="userNameProfile" style="height: 25%;display:block;width: 100%;">
                                                <h5 style="float:left;width:95%;margin: 10px;margin-bottom: 0;color: #777777;">Full Name</h5>
                                                <h5 style="float:left;width:95%;margin: 10px;margin-bottom: 0;color: #98a6ad;">{{myInfo.firstName+" "+myInfo.lastName}}</h5>
                                            </div>
                                            <div class="userNameProfile" style="height: 25%;display:block;width: 100%;">
                                                <h5 style="float:left;width:95%;margin: 10px;margin-bottom: 0;color: #777777;">Email</h5>
                                                <h5 style="float:left;width:95%;margin: 10px;margin-bottom: 0;color: #98a6ad;">{{userInfo.EmailAddress}}</h5>                                                
                                            </div>
                                            <div class="userNameProfile" style="height: 25%;display:block;width: 100%;">
                                                <h5 style="float:left;width:95%;margin: 10px;margin-bottom: 0;color: #777777;">Mobile</h5>
                                                <h5 style="float:left;width:95%;margin: 10px;margin-bottom: 0;color: #98a6ad;">{{userInfo.EmailAddress}}</h5>                                                
                                            </div>
                                       </div>
                                    </div>
                                    <div class="podcastsContainer col-md-5">
                                        <div class="podcastHeader" style="border: 1px solid #ddd;height: 15%;box-shadow: 0 0 5px 0px #969595;margin: 7px;margin-bottom: 0;">
                                            <h5 class="fontClass" style="margin: auto;margin-left: 2%;font-size: 14px;padding-left: 0;">Skills</h5>                                            
                                        </div>
                                        <div class="ImpactZoneChildDiv heightZeroClassProfile" ng-class="{ heightAnimationProfile : animationActicvate == true }" style="border: 1px solid #ddd;margin: 7px;margin-top: 0;box-shadow: 0 0 5px 0px #969595;">
                                            
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="item fullWidthHeightClass" style="overflow:auto;border-right: 1px solid #cccccc;border-left: 1px solid #cccccc;">
                            
                            <cube-Strct class="cubeParentClass" ng-show="loadingObject.showLoading" loading-Object="loadingObject"></cube-Strct>
                            <div ng-hide="loadingObject.showLoading" ng-class="{ coachColor : feedB.AsPerRole == 'Coach' , coacheeColor : feedB.AsPerRole == 'Coachee' , mentorColor : feedB.AsPerRole == 'Mentor' , menteeColor : feedB.AsPerRole == 'Mentee' }" style="display:flex;width: 95%;max-width: 95%; margin: 20px;margin-bottom:0; float: left;margin-right: 0;border: 1px solid #cccccc;border-left: 8px solid;box-shadow: none;" class="chatSender" ng-repeat="feedB in feedBackArray">
                                <div style="float:left;width: 100%;height: 100%;">
                                    <div class="chatSenderImage" style="float:left;width: 6%;">
                                        <div class="imageConversation" style="width: 100%;display: block;">
                                            <img style="width: 30px;height: 30px;border-radius: 50%;" src="~/Images/tempImage.jpg" />
                                        </div>
                                    </div>
                                    <div style="width: 94%;float:left;">
                                        <h5 class="fontClass" style="float:left;margin: 0;color: #333;text-align: left;font-size: 12px;margin-top: 2px;">Alex Ward</h5>
                                        <h5 class="fontClass" style="float:right;margin: 0;color: #aaa;text-align: left;font-size: 9px;margin-left: 7px; margin-top: 5px;font-weight: lighter;">on June 30,2016</h5>
                                        <div class="" style="float:left;width: 100%;height: 22px;">                                            
                                            @*<span ng-class="{ fillStarSpan : feedB.ratingArray.indexOf(1) > -1 }" class="starRatingSpan">&#9734</span>
                                            <span ng-class="{ fillStarSpan : feedB.ratingArray.indexOf(2) > -1 }" class="starRatingSpan">&#9734</span>
                                            <span ng-class="{ fillStarSpan : feedB.ratingArray.indexOf(3) > -1 }" class="starRatingSpan">&#9734</span>
                                            <span ng-class="{ fillStarSpan : feedB.ratingArray.indexOf(4) > -1 }" class="starRatingSpan">&#9734</span>
                                            <span ng-class="{ fillStarSpan : feedB.ratingArray.indexOf(5) > -1 }" class="starRatingSpan">&#9734</span>*@
                                            
                                            <img ng-src="{{ feedB.ratingArray.indexOf(1) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="" style="width: 15px;height: 15px;" src="~/Images/redstar.png" />
                                            <img ng-src="{{ feedB.ratingArray.indexOf(2) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="" style="width: 15px;height: 15px;" src="~/Images/redstar.png" />
                                            <img ng-src="{{ feedB.ratingArray.indexOf(3) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="" style="width: 15px;height: 15px;" src="~/Images/redstar.png" />
                                            <img ng-src="{{ feedB.ratingArray.indexOf(4) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="" style="width: 15px;height: 15px;" src="~/Images/redstar.png" />
                                            <img ng-src="{{ feedB.ratingArray.indexOf(5) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="" style="width: 15px;height: 15px;" src="~/Images/silverstar.png" />
                                        </div>
                                    </div>
                                    <h5 class="fontClass" style="font-size: 12px;float:left;margin: 0;margin-top: 6px;margin-bottom: 5px;color: #555;">{{feedB.name}}</h5>
                               </div>
                            </div>
                        </div>
                        <div class="item fullWidthHeightClass">
                            <cube-Strct class="cubeParentClass" ng-show="loadingObject.showLoading" loading-Object="loadingObject"></cube-Strct>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="applicationProfilePanel" style="position : relative;width : 100%;height: 100%;float : left;">
        <img ng-show="editRequired == 'false'" ng-click="closeProfilePopup()" style="width: 15px;height: 15px;top:5px;right: 5px;bottom: 0;position: absolute;z-index:1;cursor:pointer;" title="Cancel" src="~/Images/cancel.png" />
        <div class="profileBackgroundImageParent" style="background: url({{ myInfo.profileBackgroundImage ? myInfo.profileBackgroundImage : '../../../Images/userprofile.png'}});">
            <!--displayBackgroundImage over here -->
            <div class="profileDetailsPickerParent">
                <div class="profilePicker">

                    <img ng-hide="editRequired == 'false'" ng-click="triggerUpload()" style="width: 20px; height: 20px; float: right;margin: 3% auto;margin-right: 1%;" id="orgImage" src="~/Images/camera.png" />
                    <input type="file" name="Image" onchange="angular.element(this).scope().uploadFileFunction()" ng-model="acceptedFile" style="display: none" id="fileInputIdRv" name="filedata" />

                </div>
                <div class="profileDetails" style="background: white;">
                    <div class="profileImageParent">
                        <div class="profileImageProfile" style="background: url({{ myInfo.profileImage ? myInfo.profileImage : '../../../Images/icons/If no Profile photo.png'}}) no-repeat center; width: 85%; height: 82%;">

                            <img ng-hide="editRequired == 'false'" ng-click="triggerUpload(true)" style="width: 20px; height: 20px; background: white;float: right;margin: 1% auto;margin-right: 1%;" id="orgImage" src="~/Images/camera.png" />
                            <input type="file" onchange="angular.element(this).scope().uploadFileFunction()" ng-model="acceptedFile" style="display: none" id="fileInputIdRv" name="filedata" />
                        </div>
                    </div>
                    <div class="profileDetailsParent">
                        <div ng-init="editModeProfile = false" class="profileDetailsParents">

                            <img ng-click="editModeProfile = true" ng-hide="editModeProfile || editRequired == 'false'" style="width: 15px;float: right;height: 15px;background: white;margin: auto 0;margin-left: 1%;right: 2%;position: absolute;" src="~/Images/edit_Icon.png" />
                            <div ng-hide="editModeProfile" class="userNameProfile">
                                <h3 style="font-weight:bold;float:left;width:auto;max-width:100%;margin: auto 0;">{{ myInfo.firstName+" "+myInfo.lastName }}</h3>
                                <img ng-hide="editRequired == 'false'" style="float: left;width: 15px;height: 15px; display:none" ng-click="openValueScope()" src="~/Images/MedalImage/goldMedal.png" />
                            </div>
                            <div ng-show="editModeProfile" class="userNameProfile">
                                <div style="margin : 0;" class="formDetails">
                                    <input style="padding:2px;" ng-model="myInfo.firstName" class="inputTagStyle fontClass widthForFirstNameProfile" type="text" placeholder="FIRST NAME" />
                                    <input ng-model="myInfo.lastName" class="inputTagStyle widthForFirstNameProfile fontClass" style="padding:2px;margin-left: 2%;" type="text" placeholder="LAST NAME" />
                                </div>
                            </div>
                            @*<div ng-hide="editModeProfile" class="designationProfile">
                                    <h4 style="color: #333333;float:left;width:auto;max-width:50%;text-overflow:ellipsis;margin: auto 0;">Software Developer at LWP</h4>
                                    <img style="width: 20px;float: left;height: 20px;background: white;margin: auto 0;margin-left: 1%;" ng-src="{{organisation}}" ng-click="editDesignation = true">
                                </div>
                                <div ng-show="editModeProfile" class="designationProfile">

                                    <div style="margin: auto 0;" class="formDetails">
                                        <input style="padding:0;" class="inputTagStyle fontClass widthForFirstNameProfile" type="text" placeholder="Designation" />
                                        <input class="inputTagStyle widthForFirstNameProfile fontClass" style="padding:0;margin-left: 2%;" type="text" placeholder="Company Name" />

                                    </div>
                                </div>*@
                            <div class="emailmobileProfile">
                                <div class="subParentEmailMobile">
                                    <div style="float: left;width: 100%;height: 25px;display: flex;">
                                        <h5 style="float: left;width: 15%;margin: auto 0;color: #333333;">Email</h5>
                                        <h5 style="float: left;width: auto;max-width: 30%;text-overflow: ellipsis;margin: auto 0;color: #333333;">{{userInfo.EmailAddress}}</h5>
                                    </div>
                                    <div style="float: left;width: 100%;height: 25px;display: flex;">
                                        <h5 style="float: left;width: 15%;margin: auto 0;color: #333333;">Mobile</h5>
                                        <h5 ng-hide="editModeProfile" style="float: left;width: auto;max-width: 30%;text-overflow: ellipsis;margin: auto 0;color: #333333;">{{myInfo.mobileNumber ? '+91'+myInfo.mobileNumber : 'Not Specified'}}</h5>

                                        <input ng-show="editModeProfile" ng-model="myInfo.mobileNumber" class="inputTagStyle widthForFirstNameProfile fontClass" style="padding:5px;width: 16%;" type="text" placeholder="Mobile Number" />
                                    </div>
                                </div>

                            </div>
                            <div class="linkedInLinkProfile">

                                <img style="width: 20px;float: left;height: 20px;background: white;margin: auto 0;" src="~/Images/linkedin_square_gray.png" />
                                <h5 ng-hide="editModeProfile" style="float: left;margin: auto 0;color: #333333;margin-left: 1%;">{{myInfo.linkedInLink ? myInfo.linkedInLink : 'Not Specified'}}</h5>
                                <input ng-show="editModeProfile" ng-model="myInfo.linkedInLink" class="inputTagStyle widthForFirstNameProfile fontClass" style="padding:5px; float: left;margin: auto 0;color: #333333;margin-left: 1%;" type="text" placeholder="linkedIn link" />
                            </div>
                            <img ng-click="editModeProfile = false" ng-show="editModeProfile" style="width: 15px;float: right;height: 15px;background: white;margin: auto 0;margin-left: 1%;right: 2%;bottom: 0;position: absolute;cursor:pointer;" title="Cancel" src="~/Images/cancel.png" />
                            <img ng-click="sendDetailsToServer()" ng-show="editModeProfile" style="width: 15px;float: right;height: 15px;background: white;margin: auto 0;margin-left: 1%;right: 7%;bottom: 0;position: absolute;" title="Save" src="~/Images/save.png" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="profileDescSkillParent">
            <div class="descriptionProfileParent">
                <div class="descriptionLabel">
                    <div class="descriptionLabelParent">
                        <h5 class="descHeading">Brief Description</h5>
                        <img ng-hide="editRequired == 'false'" style="width: 15px;float: left;height: 15px;background: white;margin: auto 0;margin-left: 1%;" src="~/Images/edit_Icon.png" ng-click="editDescription =! editDescription;displayDes()">
                    </div>
                </div>
                <div ng-init="editDescription = false" class="descriptionLabelMessage">
                    <p ng-hide="editDescription" class="descriptionMessage">
                        {{myInfo.description}}
                    </p>
                    <div class="descriptionMessage" style="height: 100%;" ng-show="editDescription">
                        <textArea style="resize: none; font-size: 13px;   height: 80%;width: 100%;" placeholder="No Description Available" ng-model="myInfo.descriptiontoDisplay"></textArea>
                        <img ng-click="editDescription = false" style="width: 15px;float: right;height: 15px;margin: auto 0;margin-left: 1%;right: 2%;bottom: 0;cursor:pointer;" title="Cancel" src="~/Images/cancel.png" />
                        <img ng-click="sendDescDetailsToServer()" style=" width:15px; float right; height 15px;margin auto 0;margin-left 1%;right 7%;bottom 0;cursor:pointer;" title="Save" src="~/Images/save.png" />
                    </div>
                </div>
                <div class="skillsProfileParent">
                    <div class="descriptionLabel">
                        <div class="descriptionLabelParent">
                            <h5 class="descHeading">Skills</h5>
                            <img ng-hide="editRequired == 'false'" ng-click="editskills()" style="width: 15px;float: left;height: 15px;background: white;margin: auto 0;margin-left: 1%;" src="~/Images/edit_Icon.png">
                        </div>
                    </div>
                    <div class="skillParent" style="float: left; width: 60%;margin-left: 16%;">
                        <div class="impactZoneTag">
                            <h5 class="tagTasks fontClass coachThemeColor eliipsisClass" style="float: left;margin: 10px;margin-bottom: 0;margin-right: 0;width: 131px;height: 28px;" ng-repeat="option in topicArray">{{option.Name | uppercase}}</h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>