<div class="popupContainer" ng-class="{ coachThemeColor : role == 'Coach' , coacheeThemeColor : role == 'Coachee' , mentorThemeColor : role == 'Mentor' , menteeThemeColor : role == 'Mentee' }" ng-show="feedBack.formValue == '1'" >
    <div id="carousel-example" class="carousel slide fullWidthHeightClass" data-slide-to="carousel" data-interval="false">
        <!-- Wrapper for slides -->
        <div class="carousel-inner fullWidthHeightClass" role="listbox">
            <div class="item active fullWidthHeightClass">
                <div class="feedBackParent">
                    <img ng-click="feedBack.closeFeedBackPopup()" style="width: 15px;height: 15px;top:1%;right: 1%;bottom: 0;position: absolute;cursor:pointer" title="Close" src="~/Images/cancel.png" />
                    <div class="feedBackLabel">
                        <h5 class="resetClass fontClass">FEEDBACK FORM</h5>
                    </div>
                    <div class="feedbackPanelCom">
                        <div class="questionBox" ng-repeat="question in displayArray">
                            <div class="questionBoxparentHide" ng-class="{ questionBoxparentShow : question.showLoad == true }">
                                <div class="questionLabel">
                                    <h5 class="resetClass fontClass">{{question.name}}</h5>
                                </div>
                                <div class="questionAnswer">
                                    <div class="" style="margin: auto;display:flex;width:95%;height:95%;" ng-show="question.type == 'radio'">
                                        <input style="margin: auto;" ng-checked="selected=='yes'" ng-true-value="'yes'" ng-model="selected" title="Yes" class="radioFeedBack" type="checkbox" unchecked>
                                        <input style="margin: auto;" ng-checked="selected=='no'" ng-true-value="'no'" ng-model="selected" title="No" class="radioFeedBack radioFeedBackNo" type="checkbox" unchecked>
                                        <input style="margin: auto;" ng-checked="selected=='na'" ng-true-value="'na'" ng-model="selected" title="Not Applicable" class="radioFeedBack radioFeedBackNotApplicable" type="checkbox" unchecked>
                                    </div>
                                    <textArea class="fontClass" ng-show="question.type == 'textArea'" ng-model="question.actionValue" placeholder="Enter here Description"></textArea>
                                    <div class="ratingFeedBack" ng-show="question.type == 'rating'">
                                        <img ng-click="showRatingColor(1,question)" ng-src="{{ indexArray.indexOf(1) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="ratingStarOne" src="~/Images/silverstar.png" />
                                        <img ng-click="showRatingColor(2,question)" ng-src="{{ indexArray.indexOf(2) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="ratingStarTwo" src="~/Images/silverstar.png" />
                                        <img ng-click="showRatingColor(3,question)" ng-src="{{ indexArray.indexOf(3) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="ratingStarThree" src="~/Images/silverstar.png" />
                                        <img ng-click="showRatingColor(4,question)" ng-src="{{ indexArray.indexOf(4) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="ratingStarFour" src="~/Images/silverstar.png" />
                                        <img ng-click="showRatingColor(5,question)" ng-src="{{ indexArray.indexOf(5) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="ratingStarFive" src="~/Images/silverstar.png" />
                                    </div>
                                    <div class="ratingFeedBack" ng-show="question.type == 'checkBoxTime'">
                                        <img ng-click="showRatingColor(1,question)" ng-src="{{ indexArray.indexOf(1) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="ratingStarOne" src="~/Images/silverstar.png" />
                                        <img ng-click="showRatingColor(2,question)" ng-src="{{ indexArray.indexOf(2) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="ratingStarTwo" src="~/Images/silverstar.png" />
                                        <img ng-click="showRatingColor(3,question)" ng-src="{{ indexArray.indexOf(3) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="ratingStarThree" src="~/Images/silverstar.png" />
                                        <img ng-click="showRatingColor(4,question)" ng-src="{{ indexArray.indexOf(4) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="ratingStarFour" src="~/Images/silverstar.png" />
                                        <img ng-click="showRatingColor(5,question)" ng-src="{{ indexArray.indexOf(5) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="ratingStarFive" src="~/Images/silverstar.png" />
                                    </div>
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="pageNavigateView"style="display:flex;">
                        <button type="button" style="margin: auto auto; margin-right: 45%; padding: 6px;background-color: white;color:#333;" class="signupButton fontClass btn btn-default" ng-click="loadSlideData(1)">Next</button>
                    </div>
                </div>
            </div>
            <div class="item fullWidthHeightClass">
                <div class="feedBackParent">
                    <img ng-click="feedBack.closeFeedBackPopup()" style="width: 15px;height: 15px;top:1%;right: 1%;bottom: 0;position: absolute;cursor:pointer;" title="Close" src="~/Images/cancel.png" />
                    <div class="feedBackLabel">
                        <h5 class="resetClass fontClass">FEEDBACK FORM</h5>
                    </div>
                    <div class="feedbackPanelCom">
                        <div class="questionBox" ng-repeat="question in displayArray">
                            <div class="questionBoxparentHide" ng-class="{ questionBoxparentShow : question.showLoad == true }">
                                <div class="questionLabel">
                                    <h5 class="resetClass fontClass">{{question.name}}</h5>
                                </div>
                                <div class="questionAnswer">
                                    <div class="" style="margin: auto;display:flex;width:95%;height:95%;" ng-show="question.type == 'radio'">
                                        <input style="margin: auto;" ng-checked="question.actionValue=='yes'" ng-true-value="'yes'" ng-model="question.actionValue" title="Yes" class="radioFeedBack" type="checkbox" unchecked>
                                        <input style="margin: auto;" ng-checked="question.actionValue=='no'" ng-true-value="'no'" ng-model="question.actionValue" title="No" class="radioFeedBack radioFeedBackNo" type="checkbox" unchecked>
                                        <input style="margin: auto;" ng-checked="question.actionValue=='na'" ng-true-value="'na'" ng-model="question.actionValue" title="Not Applicable" class="radioFeedBack radioFeedBackNotApplicable" type="checkbox" unchecked>
                                    </div>
                                    <textArea ng-show="question.type == 'textArea'" ng-model="question.actionValue" placeholder="Enter here Description" style="font-size:12px"></textArea>
                                    <div class="ratingFeedBack" ng-show="question.type == 'rating'">
                                        <img ng-click="showRatingColor(1,question)" ng-src="{{ indexArray.indexOf(1) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="ratingStarOne" src="~/Images/silverstar.png" />
                                        <img ng-click="showRatingColor(2,question)" ng-src="{{ indexArray.indexOf(2) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="ratingStarTwo" src="~/Images/silverstar.png" />
                                        <img ng-click="showRatingColor(3,question)" ng-src="{{ indexArray.indexOf(3) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="ratingStarThree" src="~/Images/silverstar.png" />
                                        <img ng-click="showRatingColor(4,question)" ng-src="{{ indexArray.indexOf(4) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="ratingStarFour" src="~/Images/silverstar.png" />
                                        <img ng-click="showRatingColor(5,question)" ng-src="{{ indexArray.indexOf(5) > -1 ? 'Images/redstar.png' : 'Images/silverstar.png' }}" class="ratingStarFive" src="~/Images/silverstar.png" />
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="pageNavigateView" style="display:flex;">
                        <button type="button" style="margin: auto auto; margin-right: 5%; padding: 6px;background-color: white;color:#333;" class="signupButton fontClass btn btn-default" ng-click="loadSlideData(0,true)">Previous</button>
                        <button type="button" style="margin: auto auto; margin-left: 0%; padding: 6px;" class="signupButton fontClass btn btn-default" ng-click="feedBack.sendFeedBackDetail();feedBack.formValue = '5'">Submit</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
