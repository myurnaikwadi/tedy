            <div class="" style="float: left; width: 100%; display: flex;">                
                <button type="button" style="margin: auto auto; padding: 6px;width: 20%;" class="signupButton fontClass btn btn-default" ng-disabled="rewardsPoints.balancePoints < 10"  ng-click="feedBack.formValue = '6'">REDEEM POINTS</button>
            </div>

        </div>
    </div>
</div>

