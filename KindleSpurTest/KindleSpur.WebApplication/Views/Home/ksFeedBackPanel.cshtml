                <cube-Strct class="cubeParentClass" ng-if="loadingRewardObject.showLoading" loading-Object="loadingRewardObject"></cube-Strct>
                <div ng-show="loadingRewardObject.showLoading == false" class="" style="float: left; width: 100%;margin: auto auto;">
