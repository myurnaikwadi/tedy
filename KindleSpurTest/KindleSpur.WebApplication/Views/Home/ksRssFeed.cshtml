<div class="feedPanel">
    <div class="homeLabelfeed">
        <h5 class="fontClass" style="font-size: 16px;">KNOWLEDGE FEED</h5>
    </div>

    <div class="skillTaggingArray">
        <h5 class="tagTasks fontClass genericPointerClass" style="float: left;margin: 7px;width: 131px;height: 30px;" ng-class="{ coachThemeColor : option.selected == true }" ng-click="selectedFeedTag($index,option)" ng-repeat="option in skill">{{option.name | uppercase}}</h5>
    </div>

    <div class="feedParentArray">
        <div class="feedParentMainArray">
            <div class="feedContainRepeatDiv genericPointerClass" ng-click="loadFeedOnNextTab(feed)" ng-repeat="feed in feedContainArray">
                <div class="backgroundIamgeFeed" style="float: left;display:flex; width: 100%;height: 50%;"> @*Image*@

                </div>
                <div class="" style="float: left; width: 100%;height: 20%;">@*title*@
                    <div class="" style="width: 100%;float: left;display:flex;">
                        <p style="float: left;font-weight: 700; font-size: 14px; white-space: inherit;word-break: break-all;margin: auto;">
                            {{feed.name}}
                        </p>
                    </div>
                </div>
                <div class="" style="float: left;display:flex; width: 100%;height: 30%;">@*theory*@
                    <div class="" style="width: auto;float: left;display:flex;overflow:auto;">
                        <p style="float: left;width: 100%;height: 100%;font-size: 14px; white-space: inherit;word-break: break-all;	margin: auto;">
                            {{feed.snippet}}
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="applicationBottomStrip">
        <bottom-Main-Strip></bottom-Main-Strip>
    </div>
</div>