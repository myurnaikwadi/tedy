<!-- <monthly style="float:left;width:100%;height:100%;"></monthly> -->
<!-- monthly html code  -->
<div style="width:100%;height:7%;float:left;display:flex;">
    <div style="margin:auto;display:flex;margin-right: 1%;">
        <img class="genericPointerClass" style="margin:auto;width: 20px;height: 20px;float: left;" ng-click="dateNavigation(0)" src="~/Images/leftSideIcon.png" />
        <h5 class="monthlyWeekDayName fontClass" style="margin:auto;float: left;width: 85px;text-align: center;">{{monthDate | date : 'MMMM' }}</h5>
        <img class="genericPointerClass" style="margin:auto;width: 20px;height: 20px;float: left;" ng-click="dateNavigation(1)" src="~/Images/rightSideIcon.png" />
    </div>
</div>
<div style="width:100%;height:7%;float:left;border-top: 1px solid #ddd;border-bottom: 1px solid #ddd;">
    <div class="genericTransition" ng-repeat="day in dayName track by $index" style="height:100%;border-left: 1px solid #ddd;float: left;" ng-style="{'width': 'calc(100%/7)' }">
        <div style="width:100%;height:100%;float:left;">
            <h5 class="monthlyWeekDayName fontClass" style="float: right;">{{day.name | uppercase}}</h5>
        </div>
    </div>
</div>
<div id="monthlycontroller" style="width:100%; height:86%;float:left;">
    <div id="{{$index+'month'}}" class="monthlyCell" smooth-scroll scroll-if="{{expandIndex == $index}}" ng-repeat="dateNumberArr in monthlyArray" ng-style="dateNumberArr.styleObj" >
        <!-- ng-click="cellClickFunc(dateNumberArr,$index);ng-class="{removeClick : expandIndex != $index || expandIndex != -1}"" -->
        <div style="height:100%;width:100%;position:relative;position: relative;" ng-click="cellClickFunc(dateNumberArr,$index,iEvent)" >
            <div class="monthDayInnerContainer" ng-if="expandIndex != $index" ng-class="{ makeItPast:dateNumberArr.flag == 0, monthDayInnerBorderForToday:dateNumberArr.value=='TODAY',selectedDateOnonthly: indexClickedOn == $index && dateNumberArr.value!='TODAY'}">
                <div class="monthDayInnerRow" >                    
                    <h4 class="fontClass" style="margin: auto;margin-right: 8%;padding:0;" ng-class="{monthDayInnerRowDayNameCurrentMonth:dateNumberArr.flag==1,monthDayInnerRowDayName:dateNumberArr.flag==0,monthDayInnerRowDayNameToday:dateNumberArr.value=='TODAY'}">
                        {{dateNumberArr.dateNumber}}
                    </h4>
                </div>               
                @*<div class="monthDayInnerRow" ng-show="dateNumberArr.value=='TODAY'">
                    <h5 class="monthlyClassForTodayHeading fontClass">TODAY</h5>
                </div>*@
                <div class="monthDayInnerEventRow" ng-show="dateNumberArr.meetingArray.length>0">
                    <h5 class="eliipsisClass fontClass genericPointerClass" ng-repeat="request in dateNumberArr.meetingArray | limitTo : 3"  ng-class="{ coachThemeColor : request.Role == 'Coach' , coacheeThemeColor : request.Role == 'Coachee' , mentorThemeColor : request.Role == 'Mentor' , menteeThemeColor : request.Role == 'Mentee' }">
                        {{request.ConversationType ? (request.ConversationType+' request for skill ') : request.Subject}}
                    </h5>                    
                </div>          
                <div class="monthDayInnerEventRow" style="height: 15px;display:flex;" ng-show="dateNumberArr.meetingArray.length > 3">
                   <div style="display:flex;width:30%;margin:auto;">
                       <h4 style="margin:auto;border: 3px solid #ddd;border-radius:100%;" class="eliipsisClass fontClass"></h4>
                       <h4 style="margin:auto;border: 3px solid #ddd;border-radius:100%;" class="eliipsisClass fontClass"></h4>
                       <h4 style="margin:auto;border: 3px solid #ddd;border-radius:100%;" class="eliipsisClass fontClass"></h4>
                   </div>
                </div>
            </div>
            <div ng-if="expandIndex == $index" ng-click="$event.stopProgation()" class="monthlyExpandedDiv" style="transition:none;height:100%">
                <div class="podcastHeader">
                    <h5 style="margin: auto;margin-left: 2%;" class="fontClass ValueFeedsHeade">{{expandDay.cellDate | date : 'dd-MMM-yyyy'}}</h5>
                    <img class="genericPointerClass" style="margin: auto;margin-right: 2%;width: 20px;" src="~/Images/cancel.png" ng-click="closeExpandDateView($event)" />
                </div>
                <div class="monthlyPopupMainContainer" style="height: 90%;display:inline-block;background-color:#FAFBFB;transition:none;">
                    <div class="podcastsContainer col-md-5 col-sm-12 mosuehover" style="border-right: 1px solid lightgray;background-color: #f3f3f3;width: 33.33%;padding-right: 0;">
                        <div class="podcastHeader">
                            <h5 style="margin: auto;margin-left: 2%;" class="fontClass ValueFeedsHeade">REQUEST</h5>
                        </div>
                        <div style="height:90%;width:100%;float:left;overflow: auto;">
                            <div class="conversationList" style="height: 17%;" ng-class="{ expandedSelectedRequest  : conversation.selectedConversation == true}" ng-click="expandRequest($index,conversation,invitesRequest)" ng-repeat="conversation in invitesRequest">
                                <div class="conversationParentDiv">
                                    <div ng-show="conversation.selectedConversation == true" class="highlightedSelctedConversation"></div>
                                    <div class="imageConversation" ng-style="{ 'height' : conversation.selectedConversation == true ? '50%' : '100%'}">
                                        <img ng-if="!conversation.MeetingId" ng-class="{ activeSessionCLass :  conversation.expiredMeeting == false, closeSessionClass :  conversation.expiredMeeting == true }" ng-src="{{ conversation.Photo ? conversation.Photo  : 'Images/icons/If no Profile photo.png'}}" style="margin:auto auto;width: 40px;height: 40px;border-radius: 50%;" src="~/Images/icons/If no Profile photo.png" />
                                        <img ng-if="conversation.MeetingId" ng-class="{ activeSessionCLass :  conversation.expiredMeeting == false, closeSessionClass :  conversation.expiredMeeting == true }" ng-src="{{ conversation.FromPhoto ? conversation.FromPhoto  : 'Images/icons/If no Profile photo.png'}}" style="margin:auto auto;width: 40px;height: 40px;border-radius: 50%;" src="~/Images/icons/If no Profile photo.png" />
                                    </div>
                                    <div class="nameMessageConversation"  ng-style="{'height' : conversation.selectedConversation == true ? '50%' : '100%'}">
                                        <div ng-if="!conversation.MeetingId" style="margin:auto auto;float:left; width: 100%;height: 55%;">
                                            <h5 class="textFormatConversation fontClass" style="color:#797979;text-align: left;">{{(conversation.FirstName +" "+conversation.LastName) | uppercase }}</h5>
                                            <h5 class="textFormatConversation eliipsisClass fontClass" style="color:#aaa;text-align: left; font-size: 12px;font-weight: 100;">{{conversation.skill  | uppercase }}</h5>
                                        </div>
                                        <div ng-if="conversation.MeetingId" style="margin:auto auto;float:left; width: 100%;height: 55%;">
                                            <h5 class="textFormatConversation fontClass" style="color:#797979;text-align: left;">{{(conversation.FromFirstName +" "+conversation.FromLastName) | uppercase }}</h5>
                                            <h5 class="textFormatConversation eliipsisClass fontClass" style="text-align: left;color:#aaa; font-size: 12px;font-weight: 100;">{{conversation.SkillName  | uppercase }}</h5>
                                        </div>
                                    </div>
                                    <div class="nameMessageConversation" ng-style="{'width':'99%', 'height' : conversation.selectedConversation == true ? '50%' : '0%', 'overflow':'hidden'}">
                                        <div ng-if="conversation.MeetingId" class="meetingPersonNameStrc" style="width: 100%;height: 40%;margin-left: 0;">
                                            <button style="background-color: white;border: 1px solid #D60500;color: #D60500;font-size: 10px;padding: 5px;margin: auto;" class="signupButton fontClass" ng-click="invitesRequest.splice($index,1);updateMeeting(false,conversation)">DECLINE</button>
                                            <button style="background-color: white;border: 1px solid #06d600;color: #06d600;font-size: 10px;padding: 5px;margin: auto;" class="signupButton fontClass" ng-click="invitesRequest.splice($index,1);updateMeeting(true,conversation)">ACCEPT</button>

                                        </div>
                                        <div ng-if="!conversation.MeetingId" class="meetingPersonNameStrc" style="width: 100%;height: 40%;margin-left: 0;">
                                            <button style="background-color: white;border: 1px solid #D60500;color: #D60500;font-size: 10px;padding: 5px;margin: auto;" class="signupButton fontClass" ng-click="invitesRequest.splice($index,1);updateConversation(false, loggedEmail, conversation.EmailAddress,conversation)">DECLINE</button>
                                            <button style="background-color: white;border: 1px solid #06d600;color: #06d600;font-size: 10px;padding: 5px;margin: auto;" class="signupButton fontClass" ng-click="invitesRequest.splice($index,1);updateConversation(true, loggedEmail, conversation.EmailAddress,conversation)">ACCEPT</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="podcastsContainer col-md-5 mosuehover" style="border-right: 1px solid lightgray;width: 33.33%;background-color: #f3f3f3;">
                        <div class="podcastHeader">
                            <h5 class="fontClass" style="margin:auto;margin-left: 2%;font-size:14px;padding-left: 0;">SCHEDULED MEETINGS</h5>
                        </div>
                        <div class="ImpactZoneChildDiv" style="height: 90%;overflow: auto;">                            
                            <div class="conversationList" style="height: 17%;" ng-class="{ expandedSelectedRequest  : conversation.selectedConversation == true}" ng-click="expandRequest($index,conversation,meetingRequest)" ng-repeat="conversation in meetingRequest">
                                <div class="conversationParentDiv">
                                    <div ng-show="conversation.selectedConversation == true" class="highlightedSelctedConversation"></div>
                                    <div class="imageConversation" ng-style="{ 'height' : conversation.selectedConversation == true ? '50%' : '100%'}">                                      
                                        <img ng-class="{ activeSessionCLass :  conversation.expiredMeeting == false, closeSessionClass :  conversation.expiredMeeting == true }" ng-src="{{ conversation.FromPhoto ? conversation.FromPhoto  : 'Images/icons/If no Profile photo.png'}}" style="margin:auto auto;width: 40px;height: 40px;border-radius: 50%;" src="~/Images/icons/If no Profile photo.png" />
                                    </div>
                                    <div class="nameMessageConversation" ng-style="{'height' : conversation.selectedConversation == true ? '50%' : '100%'}">                                        
                                        <div style="margin:auto auto;float:left; width: 100%;height: 55%;">
                                            <h5 class="textFormatConversation fontClass" style="color:#797979;text-align: left;">{{(conversation.FromFirstName +" "+conversation.FromLastName) | uppercase }}</h5>
                                            <h5 class="textFormatConversation eliipsisClass fontClass" style="color:#aaa;text-align: left; font-size: 12px;font-weight: 100;">{{conversation.SkillName  | uppercase }}</h5>
                                        </div>
                                    </div>
                                    <div class="nameMessageConversation" ng-style="{'width':'99%', 'height' : conversation.selectedConversation == true ? '50%' : '0%', 'overflow':'hidden'}">
                                        <div class="meetingPersonNameStrc" style="width: 100%;height: 40%;margin-left: 0;">
                                            <button style="background-color: white;border: 1px solid #D60500;color: #D60500;font-size: 10px;padding: 5px;margin: auto;" class="signupButton fontClass" ng-click="meetingRequest.splice($index,1);updateMeeting(false,notification)">DECLINE</button>
                                            <button style="background-color: white;border: 1px solid #06d600;color: #06d600;font-size: 10px;padding: 5px;margin: auto;" class="signupButton fontClass" ng-click="meetingRequest.splice($index,1);updateMeeting(true,notification)">ACCEPT</button>
                                        </div>                                       
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="podcastsContainer col-md-5 mosuehover" style="width: 33.33%;background-color: #f3f3f3;">
                        <div class="podcastHeader">
                            <h5 class="fontClass" style="margin:auto;margin-left: 2%;font-size:14px;padding-left: 0;">UPCOMING MEETINGS</h5>
                        </div>
                        <div class="ImpactZoneChildDiv" style="height: 90%;overflow: auto;">
                            <div class="conversationList" style="height: 17%;" ng-class="{ expandedSelectedRequest  : conversation.selectedConversation == true}" ng-click="expandRequest($index,conversation,upcomingMeetingRequest)" ng-repeat="conversation in upcomingMeetingRequest">
                                <div class="conversationParentDiv">
                                    <div ng-show="conversation.selectedConversation == true" class="highlightedSelctedConversation"></div>
                                    <div class="imageConversation" ng-style="{ 'height' : conversation.selectedConversation == true ? '50%' : '100%'}">
                                        <img ng-class="{ activeSessionCLass :  conversation.expiredMeeting == false, closeSessionClass :  conversation.expiredMeeting == true }" ng-src="{{ conversation.FromPhoto ? conversation.FromPhoto  : 'Images/icons/If no Profile photo.png'}}" style="margin:auto auto;width: 40px;height: 40px;border-radius: 50%;" src="~/Images/icons/If no Profile photo.png" />
                                    </div>
                                    <div class="nameMessageConversation" ng-style="{'height' : conversation.selectedConversation == true ? '50%' : '100%'}">
                                        <div style="margin:auto auto;float:left; width: 100%;height: 55%;">
                                            <h5 class="textFormatConversation fontClass" style="color:#797979;text-align: left;">{{(conversation.FromFirstName +" "+conversation.FromLastName) | uppercase }}</h5>
                                            <h5 class="textFormatConversation eliipsisClass fontClass" style="color:#aaa; font-size: 12px;font-weight: 100;text-align: left;">{{conversation.SkillName  | uppercase }}</h5>
                                        </div>
                                    </div>
                                    <div class="nameMessageConversation" ng-style="{'width':'99%', 'height' : conversation.selectedConversation == true ? '50%' : '0%', 'overflow':'hidden'}">
                                        <div class="meetingPersonNameStrc" style="width: 100%;height: 40%;margin-left: 0;">
                                            <button style="background-color: white;border: 1px solid #D60500;color: #D60500;font-size: 10px;padding: 5px;margin: auto;" class="signupButton fontClass" ng-click="notificationData.splice($index,1);updateMeeting(false,notification)">DECLINE</button>
                                            <button style="background-color: white;border: 1px solid #06d600;color: #06d600;font-size: 10px;padding: 5px;margin: auto;" class="signupButton fontClass" ng-click="notificationData.splice($index,1);updateMeeting(true,notification)">ACCEPT</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- monthly html code  -->